Host bitbucket.org
  HostName bitbucket.org
  IdentityFile ~/.ssh/repository_rsa
  User git
  Port 22
  TCPKeepAlive yes
  IdentitiesOnly yes

Host github.com
  HostName github.com
  IdentityFile ~/.ssh/repository_rsa
  User git
  Port 22
  TCPKeepAlive yes
  IdentitiesOnly yes

Host git-codecommit.*.amazonaws.com
  User <%= @iam_ssh_pubkey_id %>
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  IdentityFile ~/.ssh/repository_rsa
