#!/usr/bin/env bash

APPLICATION_NAME=$<%= params[:eb_application_name] %>
SSH_KEY=$<%= params[:eb_ssh_key] %>
APP_PLATFORM=$<%= params[:eb_app_platform] %>
REGION=$<%= params[:eb_region] %>

echo "**********************************"
echo APPLICATION_NAME:$STG_APPLICATION_NAME
echo SSH_KEY:$STG_SSH_KEY_NAME
echo APP_PLATFORM:$STG_APP_PLATFORM
echo REGION:$AWS_DEFAULT_REGION
echo "**********************************"

eb init $APPLICATION_NAME \
        --keyname $SSH_KEY \
        --platform $APP_PLATFORM \
        --region $REGION
