#!/bin/bash
# chkconfig: 345 98 20
# description: Logging Service
# processname: td-agent-ui

NAME=td-agent-ui
USER=<%= @user %>
BIN=/usr/sbin/td-agent-ui

start() {
sudo -u $BIN start
}

stop() {
sudo -u $BIN stop
}

restart() {
stop
start
}

case "$1" in
start)
start
;;
stop)
stop
;;
restart)
restart
;;
*)
N=/etc/init.d/$NAME
echo "Usage: $N {start|stop|restart}" >&2
exit 1
;;
esac
