#!/usr/bin/env bash
RAILS_ENV=$<%= params[:eb_app_env] %>
SECRET_KEY_BASE=$<%= params[:eb_app_secret_key_base] %>
RDS_DB_NAME=$<%= params[:eb_rds_db_name] %>
RDS_USERNAME=$<%= params[:eb_rds_username] %>
RDS_PASSWORD=$<%= params[:eb_rds_password] %>
RDS_HOSTNAME=$<%= params[:eb_rds_hostname] %>
RDS_PORT=$<%= params[:eb_rds_port] %>
URL=$<%= params[:eb_url] %>
S3_BACKET=$<%= params[:eb_backet] %>

echo "**********************************"
echo RACK_ENV:$RAILS_ENV
echo SECRET_KEY_BASE:$SECRET_KEY_BASE
echo URL:$URL
echo RDS_DB_NAME:$RDS_DB_NAME
echo RDS_USERNAME:$RDS_USERNAME
echo RDS_PASSWORD:$RDS_PASSWORD
echo RDS_HOSTNAME:$RDS_HOSTNAME
echo RDS_PORT:$RDS_PORT
echo S3_BACKET:$S3_BACKET
echo "**********************************"

eb use $EB_ENVNAME
eb setenv RACK_ENV=$RAILS_ENV \
          SECRET_KEY_BASE=$SECRET_KEY_BASE \
          URL=$URL \
          RDS_DB_NAME=$RDS_DB_NAME \
          RDS_USERNAME=$RDS_USERNAME \
          RDS_PASSWORD=$RDS_PASSWORD \
          RDS_HOSTNAME=$RDS_HOSTNAME \
          RDS_PORT=$RDS_PORT \
          S3_BACKET=$S3_BACKET
